@model ReservationSystemMVC.Web.Models.ChiefDto

@{
    ViewBag.Title = "Информация об ответственном";
    Layout = "_Layout";
}

<h2>Информация об ответственном</h2>

<div id="chiefInfo">
    <dl class="row">
        <dt class="col-sm-2">ID:</dt>
        <dd class="col-sm-10">@Model.Id</dd>
        
        <dt class="col-sm-2">Name:</dt>
        <dd class="col-sm-10">@Model.Name</dd>
        
        <dt class="col-sm-2">Email:</dt>
        <dd class="col-sm-10">@Model.Email</dd>
    </dl>
</div>

<script src="~/js/chief.js"></script>